<div class="side-header">
  <div class="name">
    <%= link_to current_user.name, edit_user_path(current_user) %>
  </div>
  <div class="create">
    <%= link_to "チャットを作成する", new_room_path %>
  </div>
</div>


<div class="side-list">
  <% current_user.rooms.each do |room| %>
    <div class="rooms">
      <div class="room">
       <a href="#"><%= room.name %></a>
      </div>
   </div>
  <% end %>
</div>